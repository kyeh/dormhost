doxygen_api_docs: apidoc/html/index.html
doxygen_docs: doxygen_source_docs doxygen_api_docs

# docsource.mk is generated by preautoreconf, run by bootstrap prior to
# it running autoreconf.

@DOCUMENTATION_RULES_TRUE@code_structure.html: gen_codestructure_doc $(DIR_CONTENTS_FILES)
@DOCUMENTATION_RULES_TRUE@	./gen_codestructure_doc code_structure.html $(DIR_CONTENTS_FILES)

@DOCUMENTATION_RULES_TRUE@apidoc.pdf apidoc/html/index.html: $(APIDOC_SRC) doxygen_api_conf \
@DOCUMENTATION_RULES_TRUE@		$(srcdir)/doxygen_api_header.html_tmpl \
@DOCUMENTATION_RULES_TRUE@		$(srcdir)/doxygen_api_footer.html_tmpl
@DOCUMENTATION_RULES_TRUE@	rm -rf apidoc
@DOCUMENTATION_RULES_TRUE@	top_srcdir=$(top_srcdir) top_builddir=$(top_builddir) $(DOXYGEN) doxygen_api_conf
@DOCUMENTATION_RULES_TRUE@	cd apidoc/latex && (pool_size=750000 $(MAKE) refman.pdf||(cat refman.log;exit 1))
@DOCUMENTATION_RULES_TRUE@	mv apidoc/latex/refman.pdf apidoc.pdf
@DOCUMENTATION_RULES_TRUE@	if test "x$(PNGCRUSH)" != x ; then \
@DOCUMENTATION_RULES_TRUE@	  for png in apidoc/html/*.png ; do \
@DOCUMENTATION_RULES_TRUE@	    $(PNGCRUSH) -q $$png $${png}T && mv $${png}T $$png ; \
@DOCUMENTATION_RULES_TRUE@	  done \
@DOCUMENTATION_RULES_TRUE@	fi
@DOCUMENTATION_RULES_TRUE@	touch apidoc/html/index.html

# This dependency is required so that parallel make doesn't try to run latex
# twice simultaneously.  We touch apidoc/html/index.html after apidoc.pdf is
# created so that make doesn't attempt to regenerate it due to the dependency
# of index.html on apidoc.pdf
@DOCUMENTATION_RULES_TRUE@apidoc/html/index.html: apidoc.pdf

@DOCUMENTATION_RULES_TRUE@sourcedoc/html/index.html: $(SOURCEDOC_SRC) $(APIDOC_SRC) \
@DOCUMENTATION_RULES_TRUE@		doxygen_full_conf \
@DOCUMENTATION_RULES_TRUE@		$(srcdir)/doxygen_full_header.html_tmpl \
@DOCUMENTATION_RULES_TRUE@		$(srcdir)/doxygen_full_footer.html_tmpl
@DOCUMENTATION_RULES_TRUE@	top_srcdir=$(top_srcdir) top_builddir=$(top_builddir) $(DOXYGEN) doxygen_full_conf

@DOCUMENTATION_RULES_TRUE@.rst.html:
@DOCUMENTATION_RULES_TRUE@	$(RST2HTML) --exit-status=warning $< $@
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
