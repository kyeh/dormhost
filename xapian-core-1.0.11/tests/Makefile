	$(TESTS_ENVIRONMENT) ./apitest$(EXEEXT) -b multi

check-remote: apitest$(EXEEXT)
	$(TESTS_ENVIRONMENT) ./apitest$(EXEEXT) -b remoteprog
	$(TESTS_ENVIRONMENT) ./apitest$(EXEEXT) -b remotetcp
