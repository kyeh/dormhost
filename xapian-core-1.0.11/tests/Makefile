	$(TESTS_ENVIRONMENT) ./btreetest$(EXEEXT)
	$(TESTS_ENVIRONMENT) ./quartztest$(EXEEXT)
	$(TESTS_ENVIRONMENT) ./apitest$(EXEEXT) -b quartz

