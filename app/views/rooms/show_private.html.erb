<h1><%=h @room.street_address_1 %></h1>

<p>
  <b>Address 1:</b>
  <%=h @room.street_address_1 %>
</p>

<p>
  <b>Address 2:</b>
  <%=h @room.street_address_2 %>
</p>

<p>
  <b>City:</b>
  <%=h @room.city %>
</p>

<p>
  <b>State:</b>
  <%=h @room.state %>
</p>

<p>
  <b>Zip Code:</b>
  <%=h @room.zip %>
</p>

<p>
  <b>University:</b>
  <%=h @college.name %>
</p>

<p>
<% if @room_profile.empty? %> 
<%= link_to 'Create Room Profile', new_room_profile_path %> <br><br>
<% else %>
<%= link_to 'View Room Profile', room_profile_path(@room_profile) %> <br><br>
<% end %>
</p>

<p>
	Reservations:
<p>
<% if @transactions.empty? then %>
	No Reservations
<% else %>
	<% for transaction in @transactions %>
	<p>
		<%= link_to transaction.created_at, transaction_path(transaction) %>
	</p>
	
	<% end %>
<% end %>
</p>


<%= link_to 'Edit', edit_room_path(@room) %> |
<%= link_to 'All Rooms', rooms_path %>
