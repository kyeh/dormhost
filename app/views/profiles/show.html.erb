<h1><%= @profile_user.first_name + " " + @profile_user.last_name %></h1>
<br>
<p>
<%= image_tag @profile.photo.url %><br><br><br><br>
</p>	
<br>
<p>
	<b>College</b>:
	<%=h @profile.college.name %>
</p>

<p>
	<b>Birthdate</b>:
	<%=h @profile.birthdate %>
</p>
<p>
	<b>Graduation Date</b>:
	<%=h @profile.grad_year %>
</p>
<p>
	<b>Phone Number</b>:
	<%=h @profile.phone %>
</p>
<p>
	<b>Living Style</b>:
	<%=h @profile.living_style %>
</p>
<p>
	<b>Bedtime</b>:
	<%=h @profile.bedtime %>
</p>
<p>
	<b>Interests</b>:
	<%=h @profile.interests %>
</p>

<p>
	<b>Smoker?</b>:
	<% if @profile.smoker == true %>Yes
	<% else %> No
	<% end %>
</p>


